{
  "manifest_version": 2,
  "version": "1.0",
  "default_locale": "en",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/webvrrocks/extension",
  "icons": {
    "32": "icons/rocks-32.png",
    "64": "icons/rocks-64.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "notifications",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/vr-mode-gray.svg"
    },
    "default_title": "__MSG_extensionPageActionDefaultTitle__"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "32": "icons/rocks-32.png",
      "64": "icons/rocks-64.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
